<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.calpoly.jwmahone.firebaseverticalprototype">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".FirebaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginScreenActivity" />
        <activity android:name=".SignUpScreenActivity" />
        <activity android:name=".FirebaseApp" />
        <activity android:name=".MountainSelectionActivity"></activity>
    </application>

</manifest>